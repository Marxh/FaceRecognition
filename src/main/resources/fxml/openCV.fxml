<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="710.0" prefWidth="948.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OpenCVController">
	<children>
		<Button fx:id="homepage" mnemonicParsing="false" onAction="#goHome" prefHeight="2.0" prefWidth="86.0" style="-fx-background-radius: 0; -fx-background-color: transparent;" text="Home">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView fitHeight="19.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/home.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="reportButton" layoutX="86.0" layoutY="1.0" mnemonicParsing="false" onAction="#goReport" prefHeight="26.0" prefWidth="101.0" style="-fx-background-radius: 0; -fx-background-color: transparent;" text="Report">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView fitHeight="18.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/Report.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="settingButton" layoutX="187.0" layoutY="1.0" mnemonicParsing="false" onAction="#goSetting" prefHeight="26.0" prefWidth="84.0" style="-fx-background-radius: 0; -fx-background-color: transparent;" text="Setting">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView fitHeight="18.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/setting.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="exitButton" layoutX="271.0" layoutY="1.0" mnemonicParsing="false" onAction="#doExit" prefHeight="26.0" prefWidth="74.0" style="-fx-background-radius: 0; -fx-background-color: transparent;" text=" Exit">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
			<graphic>
				<ImageView fitHeight="18.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/exit.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Line endX="847.0" layoutX="101.0" layoutY="26.0" opacity="0.34" startX="-101.0" />
		<ImageView disable="true" fitHeight="685.0" fitWidth="949.0" layoutY="26.0" opacity="0.4" pickOnBounds="true">
			<image>
				<Image url="@../images/rainbow.png" />
			</image>
		</ImageView>
		<Hyperlink fx:id="urlOpenCV" layoutX="356.0" layoutY="678.0" onAction="#openUrlOpenCV" text="\@OpenCV" textFill="#030202" underline="true">
			<font>
				<Font name="Arial Bold" size="13.0" />
			</font>
		</Hyperlink>
		<Hyperlink fx:id="urlCMU" layoutX="482.0" layoutY="678.0" onAction="#openUrlCMU" text="\@CMU" textFill="#070606" underline="true">
			<font>
				<Font name="Arial Bold" size="13.0" />
			</font>
		</Hyperlink>
		<Hyperlink fx:id="urlHelp" layoutX="611.0" layoutY="678.0" onAction="#openUrlHelp" text="Help" textFill="#070606" underline="true">
			<font>
				<Font name="Arial Bold" size="13.0" />
			</font>
		</Hyperlink>
		<Button fx:id="captureButton" defaultButton="true" layoutX="534.0" layoutY="620.0" mnemonicParsing="false" onAction="#goCapture" prefHeight="45.0" prefWidth="86.0" style="-fx-background-color: transparent; -fx-border-color: #323232; -fx-border-radius: 0.2em;">
			<graphic>
				<ImageView fitHeight="33.0" fitWidth="83.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/tool_capture.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Label layoutX="76.0" layoutY="195.0" prefHeight="284.0" prefWidth="20.0" text="斯人若彩虹" wrapText="true" />
		<Label layoutX="43.0" layoutY="265.0" prefHeight="277.0" prefWidth="20.0" text="遇上方知有" wrapText="true" />
		<Pane layoutX="157.0" layoutY="89.0" prefHeight="514.0" prefWidth="708.0" style="-fx-border-color: #323232;">
			<children>
				<Line endX="606.0" layoutX="101.0" layoutY="68.0" startX="-100.0" />
				<ImageView fx:id="originalFrame" fitHeight="444.5" fitWidth="706.0" layoutX="1.0" layoutY="68.5" pickOnBounds="true" />
			</children></Pane>
      <Button fx:id="cameraButton" defaultButton="true" layoutX="401.0" layoutY="620.0" mnemonicParsing="false" onAction="#startCamera" prefHeight="45.0" prefWidth="107.0" style="-fx-background-color: transparent; -fx-border-color: #323232; -fx-border-radius: 0.2em;" text="Open Camera" />
	</children>
</Pane>
